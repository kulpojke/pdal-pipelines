{
        "pipeline": [
            {
                "bounds": "([-13192198.4479,-13188786.2769], [4030333.0107, 4033950.7255])",
                "filename": "https://s3-us-west-2.amazonaws.com/usgs-lidar-public/USGS_LPC_CA_LosAngeles_2016_LAS_2018/ept.json",
                "type": "readers.ept",
                "tag": "readdata",
                "threads":32,
                "spatialreference":"EPSG:3857"
            },
            {
                "type":"filters.outlier",
                "method":"radius",
                "radius":1.0,
                "min_k":4
            },
            {
                "type": "filters.reprojection",
                "in_srs":"EPSG:3857",
                "out_srs": "EPSG:26911"
            },	    
            {
                "type":"filters.hag_nn"
            },
            {
                "type":"filters.range",
                "limits":"HeightAboveGround[0:35]"
            },
	        {
		        "type":"writers.las",
       		    "filename":"SaMo3b.las"
	        },
            {
                "type":"filters.range",
                "limits":"returnnumber[1:1]"
            },
            {
             	"filename": "SaMo3b_dsm.tiff",
                "gdalopts": "tiled=yes,     compress=deflate",
                "nodata": -9999,
                "output_type": "idw",
                "resolution": 0.6,
                "type": "writers.gdal",
                "window_size": 6,
                "dimension": "Z"
            },
            {
                "filename": "SaMo3b_chm.tiff",
                "gdalopts": "tiled=yes,     compress=deflate",
                "nodata": -9999,
                "output_type": "idw",
                "resolution": 0.6,
                "type": "writers.gdal",
                "window_size": 6,
                "dimension": "HeightAboveGround"
            },
            {
                "type":"filters.range",
                "limits":"Classification[2:2]"
            },
            {
                "filename": "SaMo3b_dtm.tiff",
                "gdalopts": "tiled=yes,     compress=deflate",
                "nodata": -9999,
                "output_type": "idw",
                "resolution": 0.6,
                "type": "writers.gdal",
                "window_size": 70,
                "dimension": "Z"
            }
    ]
}
